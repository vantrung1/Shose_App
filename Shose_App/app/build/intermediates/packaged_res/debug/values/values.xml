    <style name="AppSettings_imageView_icon">
        <item name="android:layout_width">@dimen/settings_icon_width_and_height</item>
        <item name="android:layout_height">@dimen/settings_icon_width_and_height</item>
        <item name="android:layout_marginEnd">@dimen/settings_icon_end_margin</item>
        <item name="android:layout_alignParentStart">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:scaleType">fitCenter</item>
    </style>
    <style name="RelactiveSettings_settingContainer">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">?android:attr/selectableItemBackground</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="android:padding">@dimen/settings_container_padding</item>
    </style>
    <style name="TextInputLayout" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">

        <item name="boxCornerRadiusBottomEnd">20dp</item>
        <item name="boxCornerRadiusBottomStart">20dp</item>
        <item name="boxCornerRadiusTopEnd">20dp</item>
        <item name="color">@color/white</item>
        <item name="boxCornerRadiusTopStart">20dp</item>
    </style>
    <style name="Theme.Shose_App" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        
        <item name="android:statusBarColor" ns1:targetApi="l">?attr/colorPrimaryVariant</item>
        


    </style>
    <style name="easySettings_textView_title">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16dp</item>
        <item name="android:textAppearance">?android:textAppearanceMedium</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:layout_alignParentTop">true</item>
    </style>
</resources>
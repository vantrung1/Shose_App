    <dimen name="settings_container_padding">16dp</dimen>
    <dimen name="settings_divider_height">1dp</dimen>
    <dimen name="settings_icon_end_margin">16dp</dimen>
    <dimen name="settings_icon_width_and_height">24dp</dimen>
    <string name="app_name">Shose_App</string>
    <string name="hello_blank_fragment">Hello blank fragment</string>
